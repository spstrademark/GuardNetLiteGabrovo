<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/ViewFragment">

    <fragment
        android:id="@+id/ViewFragment"
        android:name="com.example.guardnet_lite_gabrovo.ViewFragment"
        android:label="@string/view_fragment_label"
        tools:layout="@layout/view_fragment">

        <action
            android:id="@+id/action_ViewFragment_to_AddFragment"
            app:destination="@id/AddFragment" />
    </fragment>
    <fragment
        android:id="@+id/AddFragment"
        android:name="com.example.guardnet_lite_gabrovo.AddFragment"
        android:label="@string/second_fragment_label"
        tools:layout="@layout/add_fragment">

        <action
            android:id="@+id/action_AddFragment_to_ViewFragment"
            app:destination="@id/ViewFragment" />
        <action
            android:id="@+id/action_AddFragment_to_SettingsFragment"
            app:destination="@id/SettingsFragment" />
    </fragment>
    <fragment
        android:id="@+id/SettingsFragment"
        android:name="com.example.guardnet_lite_gabrovo.SettingsFragment"
        android:label="SettingsFragment"
        tools:layout="@layout/settings_fragment">
        <action
            android:id="@+id/action_SettingsFragment_to_AddFragment"
            app:destination="@id/AddFragment" />
    </fragment>

</navigation>